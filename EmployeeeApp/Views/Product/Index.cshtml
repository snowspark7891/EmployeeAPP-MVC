@model List<Product>

<div>
   <table class="table table-striped table-hover">
      <thead>
         <tr>
          
            <th>ProductId</th>
            <th>ProductName</th>
            <th>Price</th>
         </tr>
      </thead>
      <tbody>
         @foreach (var product in Model)
         {
            <tr>
                   
               <td>@product.ProductId</td>
               <td>@product.ProductName</td>
               <td>@product.ProductPrice</td>
            </tr>
         }
        </tbody>
    </table>
</div>
