@model List<Product>

<div class="container">
     <table>
         <thead>
            <tr>
                <th>
                    Product Name
                </th>
                <th>
                    Productt UnitPrice
                </th>
            </tr>
         </thead>
         <tbody>
            @foreach(var product in Model)
            {
                <tr>
                    <td>
                        @product.ProductName
                    </td>
                    <td>
                        @product.ProductPrice
                    </td>
                </tr>

            }
         </tbody>
     </table>
</div>